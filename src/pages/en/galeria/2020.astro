---
import Layout from "../../../layouts/Layout.astro";
import Gallery from "../../../components/shared//Gallery.jsx";
import { t, changeLanguage } from "i18next";
import { gallery2020 } from "../../../data/constans_gallery_by_year";

changeLanguage("en");
---

<Layout title={`ITM 2024 | ${t("menu.media_hub_2")} ITM 2020}`}>
    <h1 class="font-bold text-5xl text-center text-red-600 pt-10 uppercase" >{t('menu.itm2020')}</h1>
    <Gallery
        images={gallery2020}
        galleryID={`gallery-2020`}
        client:only="react"
        class="px-4 lg:px-20"
    />
</Layout>
